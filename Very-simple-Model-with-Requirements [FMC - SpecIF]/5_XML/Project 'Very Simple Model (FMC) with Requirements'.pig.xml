        <!-- Domain-Specific Entity Classes -->
        <pig:Entity id="SpecIF:Paragraph">
            <dcterms:title xml:lang="en">Paragraph</dcterms:title>
            <dcterms:title xml:lang="de">Textabsatz</dcterms:title>
            <dcterms:title xml:lang="fr">Paragraphe</dcterms:title>
            <dcterms:description xml:lang="en">
                <p>A 'Paragraph' is an unspecified information in a document at any level.</p>
            </dcterms:description>
            <dcterms:description xml:lang="de">
                <p>Ein 'Textabschnitt' in einem Dokument auf beliebiger Ebene.</p>
            </dcterms:description>
