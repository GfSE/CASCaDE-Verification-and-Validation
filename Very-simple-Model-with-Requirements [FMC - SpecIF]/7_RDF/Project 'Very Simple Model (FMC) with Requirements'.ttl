	sh:property rdfs:comment-Shape ;
	sh:property [ 
		sh:path oslc_rm:satisfies-hasSource ;
		sh:class [ owl:unionOf( FMC:Actor FMC:State )] ;
		sh:minCount 1 ;
		sh:maxCount 1
	] ;
	sh:property [ 
		sh:path oslc_rm:satisfies-hasTarget ;
		sh:class IREB:Requirement ;
		sh:minCount 1 ;
		sh:maxCount 1
	] .

