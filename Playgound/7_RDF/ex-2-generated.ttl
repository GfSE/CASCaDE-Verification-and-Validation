#################################################################
# Data Types
#################################################################

:DT-ShortString
	rdf:type owl:class ;
	rdfs:label "String[256]" ;
	rdfs:comment "String with max. length 256" ;
	dcterms:modified "2016-05-26T08:59:00+02:00" ;
	rdfs:range xsd:string .
	# Restriction on string length still missing

:DT-Text
	rdf:type owl:class ;
	rdfs:label "Plain or formatted Text" ;
	rdfs:comment "A text string, plain, or formatted with XHTML or markdown" ;
	dcterms:modified "2021-02-14T08:59:00+02:00" ;
	rdfs:range xsd:string .

:DT-Priority
	rdf:type owl:class ;
	rdfs:label "Priorität" ;
	rdfs:comment "Enumerated values for priority" ;
	dcterms:modified "2016-05-26T08:59:00+02:00" ;
	owl:oneOf ( :V-Prio-0 :V-Prio-2 :V-Prio-4 ) .
:V-Prio-0
	a :DT-Priority ;
	rdfs:label "SpecIF:priorityHigh" .
:V-Prio-2
	a :DT-Priority ;
	rdfs:label "SpecIF:priorityMedium" .
:V-Prio-4
	a :DT-Priority ;
	rdfs:label "SpecIF:priorityLow" .

:DT-String-LE32
	rdf:type owl:class ;
	rdfs:label "String <=32" ;
	rdfs:comment "Text string with maximum length 32" ;
	dcterms:modified "2023-02-26T20:18:04.451Z" ;
	rdfs:range xsd:string .
	# Restriction on string length still missing

